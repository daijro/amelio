        }
        "OneDrive": {
            command:
            '''
            ./scripts/Windows10Debloater/UninstallOneDrive.ps1
            ./scripts/Sophia/Sophia.ps1 -Functions "OneDrive -Uninstall"
            '''
            ooconfig: {
                O001: true
            }
            default: true
