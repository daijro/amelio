    Optimization: {
        essential: {
           # sophia script commands
            sophia:
            '''
            DiagnosticDataLevel -Minimal
            ErrorReporting -Disable
            WindowsFeedback -Disable
            SigninInfo -Disable
            LanguageListAccess -Disable
            AdvertisingID -Disable
            ShareAcrossDevices -Disable
            WindowsWelcomeExperience -Hide
            WindowsTips -Disable
            SettingsSuggestedContent -Hide
            AppsSilentInstalling -Disable
            WhatsNewInWindows -Disable
            TailoredExperiences -Disable
            PowerPlan -High
            StorageSenseTempFiles -Enable
            StorageSenseRecycleBin -Enable
            DeliveryOptimization -Disable
            ThumbnailCacheRemoval -Disable
            ReservedStorage -Disable
            CortanaAutostart -Disable
            BackgroundUWPApps -Disable
            NetworkProtection -Enable
            PUAppsDetection -Enable
            DismissMSAccount
            '''
        }
        "Visual FX": {
            "Disable window contents when dragging": {
                command: Set-ItemProperty -Path "HKCU:\Control Panel\Desktop" -Name "DragFullWindows" -Type String -Value 0
                default: false
                recommend: slower systems
            }
            "Disable windows minimize animation": {
                command: Set-ItemProperty -Path "HKCU:\Control Panel\Desktop\WindowMetrics" -Name "MinAnimate" -Type String -Value 0
                default: false
                recommend: slower systems
            }
            "Disable taskbar animations": {
                command: Set-ItemProperty -Path "HKCU:\Software\Microsoft\Windows\CurrentVersion\Explorer\Advanced" -Name "TaskbarAnimations" -Type DWord -Value 0
                default: true
                recommend: yes
            }
            "Reduce menu show delay": {
                command: Set-ItemProperty -Path "HKCU:\Control Panel\Desktop" -Name "MenuShowDelay" -Type String -Value 200
                default: true
                recommend: yes
            }
            "Disable other animations": {
                command: ./scripts/DisableOtherAnimations.ps1
                default: false
                recommend: slower systems
            }
            "Disable transparency": {
                command: Set-ItemProperty -Path "HKCU:\Software\Microsoft\Windows\CurrentVersion\Themes\Personalize" -Name "EnableTransparency" -Type DWord -Value 0
                default: true
                recommend: yes
            }
        }
        "Security & Updates": {
            "Only allow security updates": {
                command: ./scripts/SecurityUpdatesOnly.ps1
                default: false
                recommend: slower systems
            }
            "Automatically adjust active hours": {
                command: ./scripts/Sophia/Sophia.ps1 -Functions "SmartActiveHours -Enable"
                default: false
                recommend: limited
            }
            "Disable Windows security": {
                command: ./scripts/DisableSecurity.ps1
                default: false
                recommend: no
            }
            "Enable Windows defender sandbox": {
                command: ./scripts/Sophia/Sophia.ps1 -Functions "DefenderSandbox -Enable"
                default: true
                recommend: yes
            }
            "Disable Windows Script Host": {
                command: ./scripts/Sophia/Sophia.ps1 -Functions "WindowsScriptHost -Disable"
                default: false
                recommend: limited
            }
            "Disable Remote Desktop": {
                command:
                '''
                Set-ItemProperty -Path "HKLM:\SYSTEM\CurrentControlSet\Control\Terminal Server" -Name "fDenyTSConnections" -Type DWord -Value 1
                Disable-NetFirewallRule -Name "RemoteDesktop*"
                '''
                default: true
                recommend: yes
            }
        }
        Performance: {
            "Use ultimate performance mode": {
                run_in_file: true
                flag: ultimateperformance
                default: true
                recommend: yes
            }
            "Use all processors": {
                command: cmd /c bcdedit /set "{current}" numproc %NUMBER_OF_PROCESSORS%
                default: true
                recommend: yes
            }
            "Unpark CPU cores": {
                command:
                '''
                Set-ItemProperty -Path "HKLM:\SYSTEM\ControlSet001\Control\Power\PowerSettings\54533251-82be-4824-96c1-47b60b740d00\0cc5b647-c1df-4637-891a-dec35c318583" -Name "ValueMax" -Type DWord -Value 0
                Set-ItemProperty -Path "HKLM:\SYSTEM\ControlSet001\Control\Power\PowerSettings\54533251-82be-4824-96c1-47b60b740d00\0cc5b647-c1df-4637-891a-dec35c318583" -Name "ValueMin" -Type DWord -Value 0
                '''
                default: true
                recommend: PC only
            }
            "Reduce boot timeout": {
                command: cmd /c bcdedit /set "{bootmgr}" timeout 3
                default: true
                recommend: yes
            }
            "No GUI boot": {
                command: cmd /c bcdedit /set "{current}" quietboot Yes
                default: true
                recommend: yes
            }
            "Fix timer resolution": {
                command:
                '''
                bcdedit /set useplatformclock no
                bcdedit /set useplatformtick yes
                bcdedit /set disabledynamictick yes
                '''
                default: true
                recommend: yes
            }
            "Disable turning off network adapters to save power": {
                command: ./scripts/Sophia/Sophia.ps1 -Functions "PCTurnOffDevice -Disable"
                default: true
                recommend: PC only
            }
            "Disable superfetch (SysMain)": {
                command:
                '''
                Stop-Service "SysMain" -WarningAction SilentlyContinue
                Set-Service "SysMain" -StartupType Disabled
                '''
                default: true
                recommend: yes
            }
            "Disable search indexing": {
                command: 
                '''
                Stop-Service "WSearch" -WarningAction SilentlyContinue
                Set-Service "WSearch" -StartupType Disabled
                '''
                default: false
                recommend: slower systems
            }
        }
        Tracking: {
            "Block tracking domains (DNS hosts list)": {
